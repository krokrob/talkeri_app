<div class="sidebar-collapse" id="sidebar">
   <%= cl_image_tag current_user.photo, class: "avatar-very-small position-absolute" %>
    <h3>SERVICES</h3>
    <div class="container font-size-regular font-weight-medium">
      <p>My events</p>
      <p>My staff</p>
      <p>My setting</p>
    </div>
    <h3>CHANNELS</h3>
    <div class="container">
        <% @channels = current_user.chatrooms.where(event_id: 1) %>
        <%@channels.each do |channel| %>
        <p class="font-size-regular font-weight-medium"><%= link_to "#{channel.name}", chatroom_path(channel) %><p>
        <% end %>
    </div>
    <h3>DIRECT CHANNELS</h3>
</div>
