<h3><%= @chatroom.name %></h3>

<p><%= @active_alert.first.content %> / Solved <%= @active_alert.first.status %></p>

<ul>
  <% @messages.each do |message| %>
    <li>
      Username: <%= message.user.first_name %>
      <%= render 'messages/message', message: message %>
    </li>
  <% end %>
</ul>

<br>
<%= render 'messages/form', chatroom: @chatroom, message: @message %>
<br>
<br>

<% @current_user_chatrooms.each do |user| %>
  <%= user.name %>
<% end %>




