<h1>My Events</h1>

<% @events.each do |event| %>
<ul>
  <li><%= event.name %>
  <h6>Start date: <%= event.start_date %></h6>
  <h6>End date: <%= event.end_date %></h6>
  <h6><%= link_to "View details", event_path(event) %></h6>
  <h6><%= link_to "Edit event", edit_event_path(event) %></h6>
  </li>
</ul>
<% end %>

<h5><%= link_to "Create a new event", new_event_path %></h5>
